@{
    ViewData["Title"] = "Sinavlar";
    Layout = "_LayoutAdmin";
    ViewData["PageTitle"] = "Sinav Işlemleri";
    ViewData["PageMainAdresse"] = "/Sinav/SinavListeleme";
    ViewData["PageMainTitle"] = "Sinav Işlemleri";
    ViewData["PageTwiceTitle"] = "Sinav Listeleme";
}
<link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.11.5/css/jquery.dataTables.css">

<div class="">
    <a asp-action="SinavEkleme" asp-controller="Sinav" class="btn  mb-1 btn-rounded btn-info">Ekle</a>
    <table id="SinavGenelTable" style="width: 100% !important;" class="table table-bordered table-hover">
        <thead>
            <tr>
                <th>#</th>
                <th>Sinav Adi</th>
                <th>Sinav Kodu</th>
                <th>Sinav Tipi</th>
                <th>Sinav Süresi</th>
                <th>Sinavı Kurum Mu Ekledi ?</th>
                <th>Sil</th>
                <th>Güncelle</th>
            </tr>
        </thead>
        <tbody id="tbody">
        </tbody>
    </table>
</div>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.11.5/js/jquery.dataTables.js"></script>
<script src="~/js/SinavList.js"></script>
