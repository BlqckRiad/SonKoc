@{
    ViewData["Title"] = "Referanslar";
    Layout = "_LayoutAdmin";
    ViewData["PageTitle"] = "Referans Işlemleri";
    ViewData["PageMainAdresse"] = "/Referanslarimiz/ReferansListeleme";
    ViewData["PageMainTitle"] = "Referans Işlemleri";
    ViewData["PageTwiceTitle"] = "Referans Ekle";
}
<script src="~/js/referansadd.js"></script>
<h2>Referans Ekleme Sayfası </h2>


    <br />
    <label>Referans Adı </label>
    <input type="text" id="ReferansAdi" class="form-control" />
   
    <br />
    <label>Referans Aciklamasi </label>
    <input type="text" id="ReferansAciklamasi" class="form-control" />
    
    <br />
    <label>Referans Olan Kişi Rolü </label>
    <input type="text" id="ReferansRolu" class="form-control" />

    <br />
    <label>Referans Puanı </label>
    <input type="number" id="ReferansPuani" class="form-control" />
   
    <br />

<label>Referans Ikonu </label>
<input type="file" id="ReferansImage" class="form-control" />

<br />

<button class="btn  mb-1 btn-rounded btn-primary" onclick="ReferansEkle()">Ekle</button>
<button class="btn  mb-1 btn-rounded btn-info" onclick="ListelemeyeDon()">Listelemeye Dön</button>


